{
	"name": "dwc-snippets",
	"displayName": "DWC snippets",
	"icon": "images/logo.jpeg",
	"description": "daily web coding snippets ",
	"publisher": "dailywebcoding",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/js.code-snippets"
			},
			{
				"language": "dotenv",
				"path": "./snippets/dotenv.code-snippets"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Chensokheng/dwc-vscode-extension.git"
	}
}
